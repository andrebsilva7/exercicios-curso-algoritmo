programa
{
	
	funcao inicio()
	{
		real n, d, t

		escreva("Digite um número: ")
		leia(n)
		d = n * 2
		escreva("O dobro de " + n + " é " + d + "\n")
		t = n / 3
		escreva("A terça parte de " + n + " é " + t)
	}
}
